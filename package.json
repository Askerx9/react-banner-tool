{
  "name": "react-banner-tool",
  "version": "0.0.1",
  "description": "tool to create banner with react",
  "main": "index.js",
  "bin": {
    "banners": "./dist/src/index.js"
  },
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json}\"",
